<h2>Sign up</h2>

<%= bootstrap_form_with(model: @user) do |f| %>
    <%= f.alert_message "Please fix errors below.", error_summary: false %>
    <%= f.email_field :email %>
    <%= f.password_field :password%>
    <%= f.password_field :password_confirmation, label: "Confirm Password"%>
    <%= f.primary 'Sign Up'%>
<% end %>